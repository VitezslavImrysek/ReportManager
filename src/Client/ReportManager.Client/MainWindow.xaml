<Window x:Class="ReportManager.Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
        xmlns:behaviors="clr-namespace:ReportManager.Client.Behaviors"
        xmlns:local="clr-namespace:ReportManager.Client"
        xmlns:vm="clr-namespace:ReportManager.Client.ViewModels"
        xmlns:views="clr-namespace:ReportManager.Client.Views"
        mc:Ignorable="d" 
        Title="ReportManager" Height="720" Width="1200">
    <Grid>
        <views:ReportView>
            <views:ReportView.DataContext>
                <vm:ReportViewModel />
            </views:ReportView.DataContext>
        </views:ReportView>
    </Grid>
</Window>
